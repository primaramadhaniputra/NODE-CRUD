<h1 class="mb-5">Contact</h1>

<a class="btn btn-primary mb-3" href="/addContact">Tambah data contact</a>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nama</th>
      <th scope="col">No HP</th>
      <th scope="col">Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% contacts.forEach((contact,index) => { %>
    <tr>
      <th scope="row"><%= ++index %></th>
      <td><%= contact.nama %></td>
      <td><%= contact.nohp %></td>
      <td>
        <a style="text-decoration: none" href="/detail/<%=contact.nama%>" class="badge bg-warning text-dark">detail</a>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
